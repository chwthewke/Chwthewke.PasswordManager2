<Window x:Class="Chwthewke.PasswordManager.DesignApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:Chwthewke.PasswordManager.App.ViewModel;assembly=Chwthewke.PasswordManager.App"
        xmlns:v="clr-namespace:Chwthewke.PasswordManager.App.View;assembly=Chwthewke.PasswordManager.App"
        Title="MainWindow"
        Height="350"
        Width="525">
    <Window.Resources>
        <DataTemplate DataType="{x:Type vm:PasswordGroupingViewModel}">
            <v:PasswordGrouping />
        </DataTemplate>
    </Window.Resources>
    <DockPanel>
        <ContentPresenter Content="{Binding PasswordGrouping}" DockPanel.Dock="Left"/>
        <Grid Background="LemonChiffon" />
    </DockPanel>
</Window>
